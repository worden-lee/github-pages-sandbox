{"uniq_tokens":{"UNIQ-WW-947887753-QINU":{"tag":"project-file","args":{"filename":"printable.Rout.pdf","linktext":"the nomogram","project":"\/usr\/local\/src\/jekyll-test\/github-pages-sandbox\/wmd_files\/2014-10-15-nomogram"}},"UNIQ-WW-1657805313-QINU":{"tag":"source-file","args":{"filename":"bf0656720ab83fcb04a5415a2b35a14d.tex-math","standalone":"yes","project":"Standalone?bf0656720ab83fcb04a5415a2b35a14d.tex-math"}},"UNIQ-WW-1527785855-QINU":{"tag":"source-file","args":{"filename":"4a8a08f09d37b73795649038408b5f33.tex-math","standalone":"yes","project":"Standalone?4a8a08f09d37b73795649038408b5f33.tex-math"}},"UNIQ-WW-1910065918-QINU":{"tag":"project-file","args":{"filename":"example.Rout.png","project":"\/usr\/local\/src\/jekyll-test\/github-pages-sandbox\/wmd_files\/2014-10-15-nomogram"}},"UNIQ-WW-340380015-QINU":{"tag":"source-file","args":{"filename":"fb7e65ce9c8bf043f622f48b5bda4db7.tex-math","standalone":"yes","project":"Standalone?fb7e65ce9c8bf043f622f48b5bda4db7.tex-math"}},"UNIQ-WW-789317271-QINU":{"tag":"source-file","args":{"filename":"92eb5ffee6ae2fec3ad71c777531578f.tex-math","standalone":"yes","project":"Standalone?92eb5ffee6ae2fec3ad71c777531578f.tex-math"}},"UNIQ-WW-1653002671-QINU":{"tag":"source-file","args":{"filename":"4a8a08f09d37b73795649038408b5f33.tex-math","standalone":"yes","project":"Standalone?4a8a08f09d37b73795649038408b5f33.tex-math"}},"UNIQ-WW-59666685-QINU":{"tag":"source-file","args":{"filename":"15e4235e578d96be98a6c3f8a346a52c.tex-math","standalone":"yes","project":"Standalone?15e4235e578d96be98a6c3f8a346a52c.tex-math"}},"UNIQ-WW-1267180961-QINU":{"tag":"source-file","args":{"filename":"4a8a08f09d37b73795649038408b5f33.tex-math","standalone":"yes","project":"Standalone?4a8a08f09d37b73795649038408b5f33.tex-math"}},"UNIQ-WW-1577677278-QINU":{"tag":"project-file","args":{"filename":"another.Rout.png","project":"\/usr\/local\/src\/jekyll-test\/github-pages-sandbox\/wmd_files\/2014-10-15-nomogram"}},"UNIQ-WW-1971903508-QINU":{"tag":"